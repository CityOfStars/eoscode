{
    "name": "eoscode",
    "displayName": "eoscode",
    "description": "VSCode extension for eosiocpp",
    "version": "0.0.1",
    "publisher": "CityOfStars",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createWAST",
        "onCommand:extension.createABI"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.createWAST",
                "title": "eoscode : Create .wast"
            },
            {
                "command": "extension.createABI",
                "title": "eoscode : Create .abi"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
